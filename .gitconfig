[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
[user]
	name = AbdelRahman Adel AbdelFatttah
	email = ad3ldev@gmail.com
[init]
	defaultBranch = main
[core]
	excludesfile = /Users/adele/.gitignore_global
    pager = delta

[interactive]
    diffFilter = delta --color-only

[delta]
    navigate = true  # use n and N to move between diff sections

[merge]
    conflictstyle = zdiff3
